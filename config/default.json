{
    "server": {
        "binding": {
            "address": "0.0.0.0",
            "port": 7777
        },
        "notificationMask": "*",
        "mirrors": [
            {
                "binding": {
                    "address": "0.0.0.0",
                    "port": 7778
                },
                "notificationMask": "",
                "writable": true
            }
        ]
    },

    "behavior": {
        "createEmptyKeysOnRequest": false,
        "deletingKeys": {
            "onlyLocalhostConnections": true
        }
    },

    "features": {
        "rpc": true,
        "dump": true,
        "delete": true,
        "fetch": true,
        "online": true,
        "regex": true,
        "identify": true,
        "options": true
    },

    "clientOptions": {
        "default": {
            "retrieveNonExisting": false,
            "lineEnding": "lf",
            "regexMode": false,
            "encoding": "utf8"
        }
    }
}
